{
  "npmScope": "guiseek",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "card": {
      "tags": []
    },
    "play-web": {
      "tags": ["type:app"]
    },
    "play-web-e2e": {
      "tags": [],
      "implicitDependencies": ["play-web"]
    },
    "paper": {
      "tags": ["type:lib"]
    },
    "play-paper": {
      "tags": ["type:app"]
    },
    "play-paper-e2e": {
      "tags": [],
      "implicitDependencies": ["play-paper"]
    },
    "chat-client": {
      "tags": ["type:feature", "side:client"]
    },
    "chat-server": {
      "tags": ["type:feature", "side:server"]
    },
    "demo-client": {
      "tags": ["type:app", "side:client"]
    },
    "demo-client-e2e": {
      "tags": [],
      "implicitDependencies": ["demo-client"]
    },
    "demo-server": {
      "tags": ["type:app", "side:server"]
    },
    "chat-data": {
      "tags": ["type:data", "side:common"]
    }
  }
}
