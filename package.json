{
  "name": "workspace",
  "version": "0.0.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guiseek/workspace.git"
  },
  "author": "Guilherme Siquinelli (https://guiseek.dev)",
  "private": false,
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "dev:watch": "eleventy --watch",
    "dev:serve": "eleventy --serve",
    "dev:start": "eleventy --serve",
    "dev:debug": "DEBUG=* eleventy",
    "clean": "rimraf dist/apps/web/dev apps/web/dev/src/_js",
    "dev:build": "npm-run-all clean build:ts build:11ty",
    "dev": "NODE_ENV=dev npm-run-all build:ts -p  watch:* -- --watch",
    "build:11ty": "eleventy --quiet",
    "watch:11ty": "eleventy --serve --quiet",
    "build:ts": "tsc --project apps/web/dev --outDir ./src/_js",
    "watch:ts": "yarn build:ts --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prerelease": "nx affected:build --all",
    "release": "nx affected --target release --all",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "scully": "scully --project web-dev",
    "scully:serve": "scully serve --project web-dev"
  },
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.1",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.2.1",
    "@angular/forms": "^10.2.1",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.5.1",
    "@nestjs/websockets": "^7.5.1",
    "@scullyio/init": "^1.0.1",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.0",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "document-register-element": "1.13.1",
    "ngx-socket-io": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "~10.1.6",
    "@angular/language-service": "^10.1.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/angular": "^10.3.1",
    "@nrwl/cli": "10.3.1",
    "@nrwl/cypress": "10.3.1",
    "@nrwl/eslint-plugin-nx": "10.3.1",
    "@nrwl/jest": "10.3.1",
    "@nrwl/nest": "^10.3.1",
    "@nrwl/node": "10.3.1",
    "@nrwl/nx-plugin": "^10.3.2",
    "@nrwl/web": "10.3.1",
    "@nrwl/workspace": "10.3.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "@types/socket.io": "^2.1.11",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "~5.0.1",
    "commitizen": "^4.2.2",
    "cypress": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "^8.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "4.2.5",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "luxon": "^1.25.0",
    "markdown-it": "^12.0.2",
    "markdown-it-anchor": "^6.0.0",
    "ng-packagr": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.4",
    "semantic-release": "17.0.7",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~4.0.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  }
}